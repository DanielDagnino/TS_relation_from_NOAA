#!/bin/bash

#------------------------------------------------------------------------------#
# For download files by specific user defnied options.
# First bash to be executed.

# To fill and remove some empty mesurements.

#------------------------------------------------------------------------------#
FI=./WOD.csv
FO=./WOD_fixed.csv

rm $FO
>$FO

#------------------------------------------------------------------------------#
# 
